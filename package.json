{
  "name": "covid19_visualizer",
  "title": "COVID-19 Visualizer",
  "url": "https://www.theworldcovid19.info/",
  "version": "1.0.0",
  "description": "An interactive COVID-19 (coronavirus) visualizer that highlights countries around the world based on the most recent cases.",
  "keywords": [
    "COVID",
    "visualizer",
    "COVID-19",
    "Coronavirus",
    "map",
    "visualization",
    "disease",
    "plague",
    "covid visualizer",
    "covidvisualizer",
    "covid-19 visualizer",
    "coronavirus visualizer"
  ],
  "scripts": {
    "test": "serverless offline --host 0.0.0.0 --httpPort 3000 & webpack-dev-server --mode development --hot --env.NODE_ENV=dev --env.API_PORT=3000 & npx cypress open",
    "start": "serverless offline --host 0.0.0.0 --httpPort 3000 & webpack-dev-server --mode development --hot --env.NODE_ENV=dev --env.API_PORT=3000",
    "build_frontend": "webpack --env.NODE_ENV=release",
    "build_and_deploy": "webpack --env.NODE_ENV=release & serverless deploy",
    "analysis_bundle": "webpack --env.NODE_ENV=analysis"
  },
  "author": "Ricky Jiang",
  "license": "ISC",
